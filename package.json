{
  "name": "rever-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate-css-types": "tailwindcss-classnames -i ./tailwind.config.js -o ./src/types/tailwind.d.ts",
    "serve": "vite preview",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --max-warnings=0",
    "prepush": "eslint src --fix && tsc && vite build",
    "docker-build": "./docker-build.sh $npm_package_name $npm_package_version $NPM_TOKEN",
    "docker-push": "./docker-push.sh $npm_package_name $npm_package_version",
    "docker-push-ci": "./docker-push-ci.sh $npm_package_name $npm_package_version",
    "serve_static": "vite preview --port 3000 --host",
    "update": "npm i -D @itsrever/design-system@latest @itsrever/returns-api-types@latest",
    "cy:open": "cypress open",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.9",
    "@reduxjs/toolkit": "^1.8.6",
    "@types/styled-components": "^5.1.26",
    "axios": "^1.1.2",
    "lottie-react": "^2.3.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "styled-components": "^5.3.6",
    "tailwindcss": "^3.1.8",
    "tailwindcss-classnames": "^3.0.6"
  },
  "devDependencies": {
    "@types/mixpanel-browser": "^2.38.0",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-react": "^2.1.0",
    "cypress": "^10.9.0 ",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "i18next": "^21.8.9",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
