{
    "name": "rever-dashboard",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host --port 3000",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "generate-css-types": "tailwindcss-classnames -i ./tailwind.config.js -o ./src/types/tailwind.d.ts",
        "serve": "vite preview",
        "test": "vitest",
        "coverage": "vitest run --coverage",
        "lint": "eslint src --max-warnings=0",
        "prepush": "eslint src --fix && tsc && vite build",
        "docker-build": "./docker-build.sh $npm_package_name $npm_package_version $NPM_TOKEN",
        "docker-push": "./docker-push.sh $npm_package_name $npm_package_version",
        "docker-push-ci": "./docker-push-ci.sh $npm_package_name $npm_package_version",
        "serve_static": "vite preview --port 3000 --host",
        "update": "yarn add @itsrever/dashboard-api@latest"
    },
    "dependencies": {
        "@auth0/auth0-react": "^1.12.0",
        "@emotion/react": "^11.10.4",
        "@emotion/styled": "^11.10.4",
        "@mui/icons-material": "^5.10.9",
        "@mui/material": "^5.11.6",
        "@mui/styled-engine-sc": "^5.10.6",
        "@reduxjs/toolkit": "^1.9.0",
        "@types/jest": "^29.2.2",
        "@types/redux-mock-store": "^1.0.3",
        "@types/styled-components": "^5.1.26",
        "apexcharts": "^3.36.3",
        "autoprefixer": "^10.4.12",
        "axios": "1.2.2",
        "clsx": "^1.2.1",
        "happy-dom": "^7.6.6",
        "i18n": "^0.15.1",
        "lodash": "^4.17.21",
        "lottie-react": "^2.3.1",
        "mixpanel-browser": "^2.45.0",
        "moment": "^2.29.4",
        "path": "^0.12.7",
        "postcss": "^8.4.18",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-apexcharts": "^1.4.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.37.0",
        "react-i18next": "^12.2.0",
        "react-redux": "^8.0.4",
        "react-router-dom": "^6.4.2",
        "react-spinners": "^0.13.7",
        "styled-components": "^5.3.6",
        "tailwindcss": "^3.1.8",
        "tailwindcss-classnames": "^3.0.6",
        "vite-plugin-svgr": "^2.2.2",
        "vite-tsconfig-paths": "^3.5.1",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@itsrever/dashboard-api": "^1.0.157",
        "@itsrever/design-system": "^1.0.48",
        "@itsrever/returns-api-types": "^0.0.84",
        "@testing-library/react": "^13.4.0",
        "@types/mixpanel-browser": "^2.38.0",
        "@types/node": "^17.0.23",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "@vitejs/plugin-react": "^2.1.0",
        "@vitest/coverage-c8": "^0.24.5",
        "@vitest/ui": "^0.25.0",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.4.0",
        "eslint-plugin-tailwindcss": "^3.5.0",
        "i18next": "^21.8.9",
        "i18next-browser-languagedetector": "^6.1.4",
        "i18next-http-backend": "^1.4.1",
        "jsdom": "^20.0.2",
        "redux-mock-store": "^1.5.4",
        "typescript": "^4.6.4",
        "vite": "^3.1.0",
        "vitest": "^0.24.5"
    }
}
